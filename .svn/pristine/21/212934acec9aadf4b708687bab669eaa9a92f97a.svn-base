package com.ciecc.fire.personal.portal.setting;

public class SettingFactory {

	private String ctx;
	private String site;
	private String siteResources;

	public SettingFactory() {
		super();
	}

	public SettingFactory(String ctx, String site, String siteResources) {
		super();
		this.ctx = ctx;
		this.site = site;
		this.siteResources = siteResources;
	}

	public String getCtx() {
		return ctx;
	}

	public void setCtx(String ctx) {
		this.ctx = ctx;
	}

	public String getSite() {
		return site;
	}

	public void setSite(String site) {
		this.site = site;
	}

	public String getSiteResources() {
		return siteResources;
	}

	public void setSiteResources(String siteResources) {
		this.siteResources = siteResources;
	}

}
